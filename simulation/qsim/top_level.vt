`timescale 1 ps/ 1 ps
module top_level_vlg_sample_tst(
	Seletor,
	X,
	Y,
	sampler_tx
);
input [3:0] Seletor;
input [5:0] X;
input [5:0] Y;
output sampler_tx;

reg sample;
time current_time;
always @(Seletor or X or Y)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

