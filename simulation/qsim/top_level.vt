begin
	repeat(6)
	begin
		X[3] = 1'b0;
		X[3] = #80000 1'b1;
		# 80000;
	end
	X[3] = 1'b0;
end 
// X[ 2 ]
initial
begin
	repeat(12)
	begin
		X[2] = 1'b0;
		X[2] = #40000 1'b1;
		# 40000;
	end
	X[2] = 1'b0;
end 
// X[ 1 ]
always
begin
	X[1] = 1'b0;
	X[1] = #20000 1'b1;
	#20000;
end 
// X[ 0 ]
always
begin
	X[0] = 1'b0;
	X[0] = #10000 1'b1;
	#10000;
end 
// Y[ 5 ]
initial
begin
	Y[5] = 1'b0;
	Y[5] = #220000 1'b1;
	Y[5] = #320000 1'b0;
	Y[5] = #320000 1'b1;
end 
// Y[ 4 ]
initial
begin
	Y[4] = 1'b0;
	# 60000;
	repeat(2)
	begin
		Y[4] = 1'b1;
		Y[4] = #160000 1'b0;
		# 160000;
	end
	Y[4] = 1'b1;
	Y[4] = #160000 1'b0;
end 
// Y[ 3 ]
initial
begin
	Y[3] = 1'b1;
	# 60000;
	repeat(5)
	begin
		Y[3] = 1'b0;
		Y[3] = #80000 1'b1;
		# 80000;
	end
	Y[3] = 1'b0;
	Y[3] = #80000 1'b1;
end 
// Y[ 2 ]
initial
begin
	Y[2] = 1'b0;
	# 20000;
	repeat(12)
	begin
		Y[2] = 1'b1;
		Y[2] = #40000 1'b0;
		# 40000;
	end
	Y[2] = 1'b1;
end 
// Y[ 1 ]
always
begin
	Y[1] = 1'b1;
	Y[1] = #20000 1'b0;
	#20000;
end 
// Y[ 0 ]
always
begin
	Y[0] = 1'b0;
	Y[0] = #10000 1'b1;
	#10000;
end 

top_level_vlg_sample_tst tb_sample (
	.Seletor(Seletor),
	.X(X),
	.Y(Y),
	.sampler_tx(sampler)
);

top_level_vlg_check_tst tb_out(
	.HEX0(HEX0),
	.HEX1(HEX1),
	.HEX2(HEX2),
	.HEX4(HEX4),
	.HEX5(HEX5),
	.HEX6(HEX6),
